    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
